:EMMO_d614f742-272e-526e-bfac-75f16b4efec4 a owl:Class ;
    rdfs:subClassOf ns1:EMMO_668fbd5b_6f1b_405c_9c6b_d6067bd0595a ;
    skos:prefLabel "TriCarboaluminate"@en ;
    ns1:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9 " is the carbonate equivalent of ettringite,"@en .

:EMMO_db71390f-9d3d-5751-8320-fe7da5e84210 a owl:Class ;
    rdfs:subClassOf :EMMO_80f7966a-4110-5f4f-8a18-df231546067f,
